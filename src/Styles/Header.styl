.app-header {
    width: 100%;
    height: 10vh;
    min-height: 55px;
    display: flex;
    padding: 0 35px;
    flex-flow: row nowrap;
    justify-content: space-between;
    align-items: center;
    background-color: $tertiary;

    input[type="search"] {
        width: 320px;
        height: 40px;
        border-radius: 5px;
        border: 1px solid black;
        padding: 0 rem(10);
        font-size: rem(20);
    }
}
